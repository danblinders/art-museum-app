{
  "name": "art-museum-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "sass": "^1.46.0",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        1,
        2
      ],
      "semi": [
        1,
        "always"
      ],
      "quotes": [
        1,
        "single"
      ],
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "no-multi-spaces": [
        1,
        {
          "ignoreEOLComments": true,
          "exceptions": {
            "Property": true
          }
        }
      ],
      "max-len": [
        1,
        {
          "code": 150
        }
      ],
      "no-trailing-spaces": [
        1,
        {
          "skipBlankLines": true
        }
      ],
      "no-multiple-empty-lines": [
        1,
        {
          "max": 1,
          "maxBOF": 1,
          "maxEOF": 0
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
